<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--需求基础信息样式-->
    <style name="workorderInfoStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/dp_16</item>
        <item name="android:paddingRight">@dimen/dp_16</item>
        <item name="android:paddingBottom">@dimen/dp_18</item>
    </style>

    <!--内容 (注意：如果不一样不要在这里修改，请在引入后内部覆盖修改)-->
    <dimen name="content_text_size">@dimen/sp_13</dimen>
    <color name="content_text_color">@color/black</color>

    <style name="content_text_style">
        <item name="android:textSize">@dimen/content_text_size</item>
        <item name="android:textColor">@color/content_text_color</item>
    </style>

</resources>